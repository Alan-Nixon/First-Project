            <script>
                function prodExplain(id) {
                    window.location.href = `/productExplain?prodId=${id}`
                }

                function addToWishList(id, from) {
                    let button = document.getElementById('addToWishList' + id + from)
                    if (button.className === 'fa fa-heart-o') {
                        button.className = "fa fa-heart"
                        button.style.color = 'red'
                    } else {
                        button.className = "fa fa-heart-o"
                    }
                    fetch(`/addToWishlist?prodId=${id}`).then((response) => {
                        Swal.fire({
                            title: (response.status === 200) ? 'ADDED TO WISHLIST' : 'ALREADY EXIST',
                            text: (response.status === 200) ? 'Item added to wishlist !' : 'Item already on wishlist ',
                            icon: (response.status === 200) ? 'success' : 'warning',
                            customClass: 'larger-sweetalert',
                            confirmButtonText: 'OK'
                        });
                    })
                }

                function addToCart(id) {
                    fetch(`/addToCart?prodId=${id}`).then((response) => {
                        let swalOptions = {
                            customClass: 'larger-sweetalert', // Apply your custom class here
                            confirmButtonText: 'OK',
                        };

                        if (response.status === 200) {
                            swalOptions.title = 'ADDED TO CART';
                            swalOptions.text = 'Item added to cart successfully!';
                            swalOptions.icon = 'success';
                        } else if (response.status === 202) {
                            swalOptions.title = 'NOT ADDED TO CART';
                            swalOptions.text = 'Maximum limit reached';
                            swalOptions.icon = 'warning';
                        } else if (response.status === 208) {
                            swalOptions.title = 'NOT ADDED TO CART';
                            swalOptions.text = 'To add to the cart, please login';
                            swalOptions.icon = 'warning';
                            swalOptions.showCancelButton = true;
                            swalOptions.confirmButtonText = 'LOGIN';
                            swalOptions.cancelButtonText = 'CANCEL';
                        } else if (response.status === 207) { 
                            swalOptions.title = " failed add to cart"
                            swalOptions.text = "cannot add to cart because product out of stock"
                            swalOptions.icon = 'warning';
                        }

                        Swal.fire(swalOptions).then((result) => {
                            if (result.isConfirmed && response.status === 208) {
                                location.href = '/login';
                            }
                        });
                    });
                }

            </script>
