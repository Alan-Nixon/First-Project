            <!-- SECTION -->
            <div class="section">
                <!-- container -->
                <div class="container">
                    <!-- row -->
                    <div class="row">
                        <!-- shop -->
                        <% shopCollections.forEach((item)=>{ %>
                            <div class="col-md-4 col-xs-6" onclick="window.location.href = '#<%= item.sectionId %>'"
                                style="cursor: pointer;">
                                <div class="shop">
                                    <div class="shop-img">
                                        <img src="<%= item.Image %>" alt="">
                                    </div>
                                    <div class="shop-body">
                                        <h3>
                                            <%= item.CategoryName %><br>Collection's
                                        </h3>
                                        <a href="#<%= item.sectionId %>" class="cta-btn">Shop now <i
                                                class="fa fa-arrow-circle-right"></i></a>
                                    </div>
                                </div>
                            </div>
                            <% }) %>

