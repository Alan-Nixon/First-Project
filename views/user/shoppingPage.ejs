                                            <!-- product -->
                                            <% newProductShop.forEach((item,index)=>{ %>
                                                    <div class="product">
                                                        <div class="product-img" onclick="prodExplain('<%= item._id %>')"
                                                            style="cursor: pointer;">
                                                            <div class="">
                                                            <img src="/productImages/<%= item.CoverImage %>" alt="">
                                                        </div>
                                                            <div class="product-label">
                                                                <% if(item.saleOffer) { %>
                                                                    <span class="sale">-<%= item.Offer %>%</span>
                                                                    <% } %>
                                                                      
                                                                    
                                                            </div>
                                                        </div>
                                                        <div class="product-body">
                                                            <p class="product-category">
                                                                <%= item.Genre %>
                                                            </p>
                                                            <h3 class="product-name"><a href="#">
                                                                    <%= item.Name %>
                                                                </a></h3>
                                                            <h4 class="product-price">
                                                               <% if (item.Offer === 0) { %>
                                                                <%= item.Price %> <del class="product-old-price">
                                                                    <%= item.Price %>
                                                                <% } else { %>
                                                                    <%= (item.Price * item.Offer)/100 %> <del class="product-old-price">
                                                                        <%= item.Price %>
                                                                    </del>
                                                                    <% } %>
                                                            </h4>
                                                            <div class="product-rating">
                                                                <% for(let i=0;i<item.Rating;i++ ){ %>
                                                                    <i class="fa fa-star"></i>
                                                                    <% } %>
                                                            </div>
